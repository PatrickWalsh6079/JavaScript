// OBJECTS


// create Object and extract info from it
var person = {
    firstName: "Joe",
    lastName: "Schmoe",
    country: "Norway"
};
// display the firstName element
console.log(person.firstName);


// create Object and display elements of Object inside text
var person = {
    name: "Joe", age: 50
};
console.log(person.name + " is " + person.age);

