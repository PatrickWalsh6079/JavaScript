// MATH

// create random number between 0-1
var r = Math.random();
console.log(r);

// return largest of two numbers
var x = Math.max(10,20);
console.log(x);

// round number to nearest integer
// rounds up at .5
var x = Math.round(5.5);
console.log(x);

// find square root of number
var x = Math.sqrt(9);
console.log(x);
